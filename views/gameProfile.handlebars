{{!-- These will be the game cards that show up for each game  --}}
<div class="gameProfileSection">
	<div class="row">
		<div class="gameCard">
			<div class="cardHeader">
				<h4 class="gameTitle"> {{game.game_name}} </h4>
				<h4 class="gameRating">
					{{#if game.reviews}}
						{{averageOut game.reviews}}
						out of out of
						{{game.reviews.length}}
						reviews â˜…
					{{else}}No reviews yet
					{{/if}}
				</h4>
			</div>
			<div class="gameImg">
				<img src="/images/{{game.game_picture}}.jpg" />
			</div>
			<div class="gameProfile">
				<p>{{game.game_desc}}</p>
			</div>
		</div>
	</div>
</div>

<link rel="stylesheet" href="/css/style.css" />
<script src="/js/register.js"></script>
<script src="/js/logout.js"></script>